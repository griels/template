          <wx:font wx:val="Calibri" />
        </w:listPr>
        <w:rPr>
          <w:sz-cs w:val="20" />
        </w:rPr>
      </w:pPr>
      <w:r><w:rPr></w:rPr><w:t>Modified and ported YAJSW (Yet Another Java Service Wrapper) to the Maven Build system to facilitate cross platform service installation for the UAS Launcher (for running Aculab Cloud applications).</w:t></w:r>

    </w:p>
    <w:p wsp:rsidR="00C146CA" wsp:rsidRDefault="009452CA"
	wsp:rsidP="00C146CA">
      <w:pPr>
                <w:keepNext />
        <w:pStyle w:val="ListParagraph" />
