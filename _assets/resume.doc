          <wx:font wx:val="Calibri" />
        </w:listPr>
        <w:rPr>
          <w:sz-cs w:val="20" />
        </w:rPr>
      </w:pPr>
      <w:r><w:rPr></w:rPr><w:t>Authored Maven build configuration files for compilation, packaging, documentation, obfuscation (with ProGuard) and deployment.</w:t></w:r>

    </w:p>
    <w:p wsp:rsidR="00C146CA" wsp:rsidRDefault="009452CA"
	wsp:rsidP="00C146CA">
      <w:pPr>
                <w:keepNext />
        <w:pStyle w:val="ListParagraph" />
        <w:listPr>
          <w:ilvl w:val="0" />
          <w:ilfo w:val="1" />
          <wx:t wx:val="•" />
          <wx:font wx:val="Calibri" />
        </w:listPr>
        <w:rPr>
          <w:sz-cs w:val="20" />
        </w:rPr>
      </w:pPr>
      <w:r><w:rPr></w:rPr><w:t>Employed a test-driven development approach from the outset, ensuring correct behaviour throughout the evolution of the library.</w:t></w:r>

    </w:p>
    <w:p wsp:rsidR="00C146CA" wsp:rsidRDefault="009452CA"
	wsp:rsidP="00C146CA">
      <w:pPr>
                <w:keepNext />
        <w:pStyle w:val="ListParagraph" />
