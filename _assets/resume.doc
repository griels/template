          <wx:font wx:val="Calibri" />
        </w:listPr>
        <w:rPr>
          <w:sz-cs w:val="20" />
        </w:rPr>
      </w:pPr>
      <w:r><w:rPr></w:rPr><w:t>Wrote doclet code to generate XML and JavaScript/JQuery code to process this output into documentation web pages for the API.</w:t></w:r>

    </w:p>
    <w:p wsp:rsidR="00C146CA" wsp:rsidRDefault="009452CA"
	wsp:rsidP="00C146CA">
      <w:pPr>
                <w:keepNext />
        <w:pStyle w:val="ListParagraph" />
