          <wx:font wx:val="Calibri" />
        </w:listPr>
        <w:rPr>
          <w:sz-cs w:val="20" />
        </w:rPr>
      </w:pPr>
      <w:r><w:rPr></w:rPr><w:t>Coordinated development roles within our test framework software, ensuring interface changes were correctly addressed by affected developers.</w:t></w:r>

    </w:p>
    <w:p wsp:rsidR="00C146CA" wsp:rsidRDefault="009452CA"
	wsp:rsidP="00C146CA">
      <w:pPr>
                <w:keepNext />
